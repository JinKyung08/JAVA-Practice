package inheritanceExam;

public class Student {
// 학번, 이름, 나이 - private
	private String ssn; //  외부에서 접근 X, 내부에서는 자유롭게 접근 가능
	private String name;
	private int age;
	
// 생성자를 통해 초기화 
	public Student() {
		ssn = "123";
		name = "홍길동";
		age = 20;
		
//		this.ssn = "123";		//구분가능할 때 this 안적어도됨
//		this.name = "홍길동";
//		this.age = 20;
	}
	
// 메서드 => 학번과 이름과 나이가 출력
	

}
