package arrayExam;

public class CarTest01 {
	//특성 속성 ( 제조사, 모델명, 색상, 최대속력)
	//필드
	String company ;
	String model ;
	String color = "노랑";
	int speed = 250 ;
	static int maxSpeed = 450;
	//생성자
	public CarTest01 () {}
	
	//메서드 - 동작, 작동방식, 정보, ...
	public void carInfo() {  //인스턴스 메서드
		System.out.println("모델명은 " + model + "," + "색상은 " + color);
	}
	
	public static void carShow() {
		System.out.println("최대속력= " + maxSpeed);
	}
	
}
