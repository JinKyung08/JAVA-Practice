package polymorphism;

public class CastingExam01 {

	public static void main(String[] args) {
		CastA ca = new CastA();
		System.out.println(ca.m);
		ca.showA();
		System.out.println();
		
		CastB cb = new CastB();
		System.out.println(cb.m);
		cb.showA();
		System.out.println(cb.n);
		cb.showB();
	}

}
