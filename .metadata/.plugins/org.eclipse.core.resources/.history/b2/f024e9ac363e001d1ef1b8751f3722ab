package polymorphism;

public class CastingExam01 {

	public static void main(String[] args) {
		CastA ca = new CastA();
		System.out.println("CastA m : " + ca.m);
		ca.showA();
		System.out.println();
		
		CastB cb = new CastB();
		System.out.println("CastA m : " + cb.m);
		cb.showA();
		System.out.println("CastB n : " + cb.n);
		cb.showB();
	}

}
