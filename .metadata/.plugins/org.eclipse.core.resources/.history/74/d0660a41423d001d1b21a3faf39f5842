package arrayExam;

public class Student {

	public static void main(String[] args) {
		int [] lotto = new int[45];
		
		for(int i=0; i < lotto.length; i++) {
			lotto[i]=i+1;
		}
		
		for(int i=0; i<lotto.length; i++) {
			int n = (int)(Math.random()*45)+1;
			int temp = 0;
			
			temp = lotto[i];
			lotto[i] = lotto[n];
			lotto[n] = temp;
			
		}
		for (int i= 0; i<6; i++) {
			System.out.println(lotto[i]);
		}
		
	}

}
