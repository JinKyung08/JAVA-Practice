package inheritanceExam;

public class Bird {
	String eye;
	String mouth;
	String name;
	String wings;
	//생성자
	public Bird() {}
	public Bird(String eye, String mouth, String name, String wings) {
		super(); //부모 생성자 호출 , 부모먼저 초기화
		this.eye = eye;
		this.mouth = mouth;
		this.name = name;
		this.wings = wings;
	}
	public void showBird() {
		System.out.println("eye : " + eye);
		System.out.println("mouth : " + mouth);
		System.out.println("name : " + name);
		System.out.println("wings : " + wings);
	}
	public void eat() {
		System.out.println("부리로 모이를 먹는다.");
	}
	public void sleep() {
		System.out.println("새는 나무 위에서 잔다.");
	}
	public void cry() {
		System.out.println("새는 짹짹 운다.");
	}
	public void fly() {
		System.out.println("새가 날아간다");
	}
	
}
