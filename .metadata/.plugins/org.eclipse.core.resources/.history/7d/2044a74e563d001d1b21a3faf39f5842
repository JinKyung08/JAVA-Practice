package arrayExam;

public class CarTest01Main {

	public static void main(String[] args) {
		//객체생성
		CarTest01 myCar = new CarTest01();
		CarTest01 momCar = new CarTest01();
		
		//객체사용
		myCar.company = "현대";
		myCar.model = "아반떼";
		
		myCar.carInfo();
		
		momCar.company = "기아";
		momCar.model = "K7";
		
		
		
		
	}

}
