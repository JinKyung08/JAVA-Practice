package interfaceExam;

public class AnimalMain {

	public static void main(String[] args) {
			
			Cat cat = new Cat();
			Dog dog = new Dog();
			Bird bird = new Bird();
			
			cat.cry();
			dog.cry();
			bird.cry();
			
			Animal[] ani = {new Cat(), new Dog(), new Bird()};
			
			for(Animal a : ani) {
				System.out.println(a);
			}
	}

}
