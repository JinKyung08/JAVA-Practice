package classExam.studentArray;

public class Student {
		//인스턴스필드
		String name; //이름 
		String sn; //학번
		Subject[] subject; // 수강과목별 점수  // Subject클래스에서 가져온것
		
		//생성자
		public Student() { } //기본생성자
		
		public Student(String name,String sn,int subjectCount) {
			this.name = name; // 이름 초기화
			this.sn=sn; // 학번 초기화
			this.subject = new Subject[subjectCount]; // 입력받은 수강 과목수에 맞게 배열 공간 확보
		}
		//메서드
		public void addSubject(String subjectName,int subScore) {
			Subject newSub = new Subject(subjectName, subScore);
			
			
			
			
		}
		
		
		
		
		
		
		
		
		
}
